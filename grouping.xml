    <classes>
       <class name="testCases.TC_RF_001_AccountRegistration"/> 
       <class name="testCases.TC_LF_002_LoginTest"/>
      <!--<class name="testCases.TC_LF_003_LoginTestDDT"/>-->
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
