<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Grouping Suite">

	<groups>
		<run>
			<include name="Master"/>
			<!--<include name="Sanity"/>-->
			<!-- <exclude name="Regression"/>-->
		</run>
	</groups>
	
	<listeners>
